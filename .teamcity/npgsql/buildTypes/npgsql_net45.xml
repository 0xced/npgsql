<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="28679227-56e2-42c8-8ab4-c6c148ccff60" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>.NET 4.5</name>
  <description>Npgsql targeting .NET 4.5</description>
  <settings ref="npgsql_dotnet_build">
    <options>
      <option name="allowExternalStatus" value="true" />
      <option name="artifactRules" value="src/Npgsql/bin/%system.Configuration%/**/*.dll =&gt; Npgsql/bin/%system.Configuration%&#xA;src/Npgsql/bin/%system.Configuration%/**/*.pdb =&gt; Npgsql/bin/%system.Configuration%&#xA;src/Npgsql/bin/%system.Configuration%/**/*.xml =&gt; Npgsql/bin/%system.Configuration%&#xA;src/Npgsql.EntityFramework/bin/%system.Configuration%/Npgsql.EntityFramework.dll =&gt; Npgsql.EntityFramework/bin/%system.Configuration%&#xA;src/Npgsql.EntityFramework/bin/%system.Configuration%/Npgsql.EntityFramework.pdb =&gt; Npgsql.EntityFramework/bin/%system.Configuration%&#xA;src/Npgsql.EntityFramework/bin/%system.Configuration%/Npgsql.EntityFramework.xml =&gt; Npgsql.EntityFramework/bin/%system.Configuration%&#xA;src/Npgsql.EntityFramework/bin/Legacy-%system.Configuration%/Npgsql.EntityFrameworkLegacy.dll =&gt; Npgsql.EntityFramework/bin/Legacy-%system.Configuration%&#xA;src/Npgsql.EntityFramework/bin/Legacy-%system.Configuration%/Npgsql.EntityFrameworkLegacy.pdb =&gt; Npgsql.EntityFramework/bin/Legacy-%system.Configuration%&#xA;src/Npgsql.EntityFramework/bin/Legacy-%system.Configuration%/Npgsql.EntityFrameworkLegacy.xml =&gt; Npgsql.EntityFramework/bin/Legacy-%system.Configuration%" />
    </options>
    <disabled-settings>
      <setting-ref ref="RUNNER_1" />
      <setting-ref ref="RUNNER_11" />
    </disabled-settings>
    <parameters>
      <param name="system.Configuration" value="Release-net45" />
    </parameters>
    <build-runners order="RUNNER_15, RUNNER_7, RUNNER_9, RUNNER_2, RUNNER_10, RUNNER_1, RUNNER_11, RUNNER_14">
      <runner id="RUNNER_15" name="GitVersion" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="GitVersion" />
          <param name="command.parameters" value="/output buildserver /UpdateAssemblyInfo src\CommonAssemblyInfo.cs" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_9" name="Restore packages DNU (ASP.NET 5.0)" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[set path=%%path%%;C:\Windows\System32\WindowsPowerShell\v1.0
c:\users\teamcity_agent\.dnx\bin\dnvm.cmd exec default dnu restore]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_10" name="Build EF7" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[set path=%%path%%;C:\Windows\System32\WindowsPowerShell\v1.0
c:\users\teamcity_agent\.dnx\bin\dnvm.cmd exec default dnu build --configuration Release]]></param>
          <param name="teamcity.build.workingDir" value="src/Npgsql.EntityFramework7" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_11" name="Test EF7" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[set path=%%path%%;C:\Windows\System32\WindowsPowerShell\v1.0
c:\users\teamcity_agent\.dnx\bin\dnvm.cmd exec default dnx . test]]></param>
          <param name="teamcity.build.workingDir" value="test/Npgsql.EntityFramework7.FunctionalTests" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_14" name="Revert CommonAssemblyInfo" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="git" />
          <param name="command.parameters" value="checkout src\CommonAssemblyInfo.cs" />
          <param name="teamcity.step.mode" value="execute_if_failed" />
        </parameters>
      </runner>
      <runner id="RUNNER_19" name="Clear nupkgs" type="simpleRunner">
        <parameters>
          <param name="script.content" value="if not exist nupkgs mkdir nupkgs" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_20" name="New build step" type="jb.nuget.pack">
        <parameters>
          <param name="nuget.pack.as.artifact" value="true" />
          <param name="nuget.pack.include.sources" value="true" />
          <param name="nuget.pack.output.clean" value="true" />
          <param name="nuget.pack.output.directory" value="nupkgs" />
          <param name="nuget.pack.project.dir" value="as_is" />
          <param name="nuget.pack.properties" value="Configuration=Release" />
          <param name="nuget.pack.specFile"><![CDATA[src\Npgsql\Npgsql.nuspec
src\Npgsql.EntityFramework\Npgsql.EntityFramework.nuspec
src\Npgsql.EntityFramework\Npgsql.EntityFrameworkLegacy.nuspec]]></param>
          <param name="nuget.pack.version" value="%GitVersion.NuGetVersion%" />
          <param name="nuget.path" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="nugetCustomPath" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="nugetPathSelector" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_21" name="NuGet Pack EF7" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[set path=%%path%%;C:\Windows\System32\WindowsPowerShell\v1.0
c:\users\teamcity_agent\.dnx\bin\dnvm.cmd exec default dnu pack --configuration Release]]></param>
          <param name="teamcity.build.workingDir" value="src/Npgsql.EntityFramework7" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_22" name="Publish nugets to myget" type="jb.nuget.publish">
        <parameters>
          <param name="nuget.path" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="nuget.publish.files" value="nupkgs/*.nupkg" />
          <param name="nuget.publish.source" value="https://www.myget.org/F/npgsql-unstable" />
          <param name="nugetCustomPath" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="nugetPathSelector" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="secure:nuget.api.key" value="zxxca89988ce5b6f9ad529c572cd56e162ccda4fdff046b0cdb8bff3560c8e35dcbda3e64637e4ca2f1" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

